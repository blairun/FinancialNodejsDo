{
  "name": "financial_vue_server",
  "version": "0.1.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "serve-app-test": "set DB_ENV=dbTest&& nodemon app.js",
    "serve-app-live": "nodemon app.js",
    "plaid-start": "set PLAID_ENV=development&& node scripts/plaidServer.js",
    "plaid-start-sand": "set PLAID_ENV=sandbox&& node scripts/plaidServer.js",
    "seed-test-db": "set DB_ENV=dbTest&& node seed",
    "--Digital-Ocean--": "DO env variable DB_ENV is set to dbTest so app always uses Plaid test environment",
    "start": "node app.js",
    "start-test": "set DB_ENV=dbTest&& node app.js",
    "start_original_DO": "node ./bin/www"
  },
  "scriptComments": {
    "test-cron": "node cron.js",
    "get-accounts": "PLAID_ENV=development node tests/getAccounts.js",
    "update-balances": "PLAID_ENV=development node tests/updateBalances.js",
    "update-transactions": "PLAID_ENV=development node tests/updateTransactions.js",
    "space-0": "",
    "get-accounts-sand": "PLAID_ENV=sandbox node tests/getAccounts.js",
    "update-balances-sand": "PLAID_ENV=sandbox node tests/updateBalances.js",
    "update-transactions-sand": "PLAID_ENV=sandbox node tests/updateTransactions.js",
    "space-1": "",
    "serve-app-forever": "forever start --watch true app.js",
    "plaid-start-forever": "forever start scripts/plaidServer.js",
    "plaid-stop-forever": "forever stop scripts/plaidServer.js",
    "space-2": "",
    "test-plaid": "node tests/plaid.js",
    "test-sqlite": "node tests/sqlite3.js",
    "test-sequelize": "node tests/sequelize.js",
    "space-3": "",
