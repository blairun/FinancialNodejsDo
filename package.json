    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dayjs": "^1.9.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "googleapis": "59.0.0",
    "joi": "^17.2.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "plaid": "^6.0.0",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "pg": "8.3.3"
